{
	"globals": {
		"debug": true,
		"__SYSCALL": true,
		"runtime": true,
		"$$": true,
		"PERSISTENCE": true,
		"performance": true
	},
	"env": {
		"es6": true,
		"node": true,
		"browser": false
	},
	"extends": "airbnb-base",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "script",
		"ecmaFeatures": {
			"modules": false,
			"defaultParams": true,
			"classes": true,
			"arrowFunctions": true,
			"blockBindings": true,
			"forOf": true,
			"spread": true,
			"templateStrings": true
		}
	}
}